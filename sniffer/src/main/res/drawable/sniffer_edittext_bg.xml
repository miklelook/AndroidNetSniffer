<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_enabled="false">
            <nine-patch
                android:src="@drawable/sniffer_text_alpha"
                android:tint="@color/sniffer_list_divider" />
        </item>
        <item
            android:state_focused="false"
            android:state_pressed="false">
            <nine-patch
                android:src="@drawable/sniffer_text_alpha"
                android:tint="@color/sniffer_list_divider" />
        </item>
        <item>
            <nine-patch
                android:src="@drawable/sniffer_text_alpha"
                android:tint="@color/sniffer_console_text" />
        </item>
    </selector>
</inset>